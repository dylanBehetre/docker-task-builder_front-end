CREATE TABLE priority (
  uuid NUMBER NOT NULL,
  command TEXT NOT NULL,
  order_task NUMBER NOT NULL,
  order_queue NUMBER NOT NULL,
  PRIMARY KEY(order_queue, order_task)
);

CREATE TABLE normal (
  uuid NUMBER NOT NULL,
  command TEXT NOT NULL,
  order_task NUMBER NOT NULL,
  order_queue NUMBER NOT NULL,
  PRIMARY KEY(order_queue, order_task)
);
